{
  "name": "@codesordinatestudio/chunk_uploader",
  "version": "1.0.1",
  "description": "A configurable chunk uploader package for handling large file uploads in a distributed system",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesordinatestudio/chunk_uploader"
  },
  "scripts": {
    "build": "tsup",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "bun test && bun run build"
  },
  "keywords": [
    "chunk-uploader",
    "file-upload",
    "large-files",
    "distributed-system",
    "upload",
    "progressive-upload",
    "bun",
    "nodejs"
  ],
  "author": "codesordinatestudio",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "bullmq": "^5.56.0",
    "ioredis": "^5.6.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ]
}
